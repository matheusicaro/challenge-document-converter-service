{
  "name": "challenge-document-converter-service",
  "version": "1.0.0",
  "description": " API to convert documents between three different format",
  "license": "MIT",
  "author": "Matheus Icaro",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/app",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "precommit": "lint-staged",
    "prepare": "sh ./scripts/prepare.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "10.4.15",
    "@nestjs/core": "10.4.15",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/platform-fastify": "^10.4.15",
    "@nestjs/swagger": "^8.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "husky": "^9.1.7",
    "i": "^0.3.7",
    "lint-staged": "^15.2.11",
    "matheusicaro-node-framework": "^1.0.5",
    "npm": "^10.9.2",
    "reflect-metadata": "0.2.2",
    "rimraf": "5.0.10",
    "rxjs": "7.8.1",
    "swagger-ui-express": "^5.0.1",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.9",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.15",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/multer": "1.4.12",
    "@types/node": "22.10.0",
    "@types/supertest": "6.0.2",
    "@types/xml2json": "^0.11.6",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "supertest": "6.3.4",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.2"
  }
}
